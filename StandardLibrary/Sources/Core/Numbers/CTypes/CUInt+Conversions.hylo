        &self = UInt64(value: Builtin.fromcint_cuint_i64(n.value))
    }
}
public extension Int {
    public init(truncating_or_extending n: CUInt) {
        // assert(n <= Int.max())
        &self = Int(value: Builtin.fromcint_cuint_word(n.value))
    }
}
public extension UInt {
    public init(truncating_or_extending n: CUInt) {
        &self = UInt(value: Builtin.fromcint_cuint_word(n.value))
    }
}

public extension CUInt {
    public init(_ n: UInt8) {
        &self = CUInt(value: Builtin.tocint_u_i8_cuint(n.value))
    }

    public init(_ n: Int8) {
        // assert (n >= 0)
        &self = CUInt(value: Builtin.tocint_s_i8_cuint(n.value))
    }

    public init(_ n: UInt32) {
        &self = CUInt(value: Builtin.tocint_u_i32_cuint(n.value))
    }

    public init(_ n: Int32) {
        // assert (n >= 0)
        &self = CUInt(value: Builtin.tocint_s_i32_cuint(n.value))
    }

    public init(_ n: UInt64) {
        // assert (n <= UInt64(self.max()))
        &self = CUInt(value: Builtin.tocint_u_i64_cuint(n.value))
    }

    public init(_ n: Int64) {
        // assert (n >= 0 && n <= Int64(self.max()))
        &self = CUInt(value: Builtin.tocint_s_i64_cuint(n.value))
    }

    public init(_ n: UInt) {
        // assert (n <= UInt(self.max()))
        &self = CUInt(value: Builtin.tocint_u_word_cuint(n.value))
    }

    public init(_ n: Int) {
        // assert (n >= 0 && n <= Int(self.max()))
        &self = CUInt(value: Builtin.tocint_s_word_cuint(n.value))
    }
}