public extension Int8 {
    public init(truncating_or_extending n: CUInt) {
        // assert(n <= Int8.max())
        &self = Int8(value: Builtin.fromcint_cuint_i8(n.value))
    }
}
public extension UInt8 {
    public init(truncating_or_extending n: CUInt) {
        // assert(n <= UInt8.max())
        &self = UInt8(value: Builtin.fromcint_cuint_i8(n.value))
    }
}
public extension Int32 {
    public init(truncating_or_extending n: CUInt) {
        // assert(n <= Int32.max())
        &self = Int32(value: Builtin.fromcint_cuint_i32(n.value))
    }
}
public extension UInt32 {
    public init(truncating_or_extending n: CUInt) {
