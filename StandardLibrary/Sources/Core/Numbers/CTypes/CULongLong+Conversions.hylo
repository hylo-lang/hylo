public extension CULongLong {
    public init(_ n: UInt8) {
        &self = CULongLong(value: Builtin.tocint_u_i8_culonglong(n.value))
    }

    public init(_ n: Int8) {
        // assert (n >= 0)
        &self = CULongLong(value: Builtin.tocint_s_i8_culonglong(n.value))
    }

    public init(_ n: UInt32) {
        &self = CULongLong(value: Builtin.tocint_u_i32_culonglong(n.value))
    }

    public init(_ n: Int32) {
        // assert (n >= 0)
        &self = CULongLong(value: Builtin.tocint_s_i32_culonglong(n.value))
    }

    public init(_ n: UInt64) {
        &self = CULongLong(value: Builtin.tocint_u_i64_culonglong(n.value))
    }

    public init(_ n: Int64) {
        // assert (n >= 0)
        &self = CULongLong(value: Builtin.tocint_s_i64_culonglong(n.value))
    }

    public init(_ n: UInt) {
        &self = CULongLong(value: Builtin.tocint_u_word_culonglong(n.value))
    }

    public init(_ n: Int) {
        // assert (n >= 0)
        &self = CULongLong(value: Builtin.tocint_s_word_culonglong(n.value))
    }
}