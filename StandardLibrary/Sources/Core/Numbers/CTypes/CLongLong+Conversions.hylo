public extension CLongLong {
    public init(_ n: UInt8) {
        &self = CLongLong(value: Builtin.tocint_u_i8_clonglong(n.value))
    }

    public init(_ n: Int8) {
        &self = CLongLong(value: Builtin.tocint_s_i8_clonglong(n.value))
    }

    public init(_ n: UInt32) {
        &self = CLongLong(value: Builtin.tocint_u_i32_clonglong(n.value))
    }

    public init(_ n: Int32) {
        &self = CLongLong(value: Builtin.tocint_s_i32_clonglong(n.value))
    }

    public init(_ n: UInt64) {
        // assert (n <= UInt64(self.max()))
        &self = CLongLong(value: Builtin.tocint_u_i64_clonglong(n.value))
    }

    public init(_ n: Int64) {
        &self = CLongLong(value: Builtin.tocint_s_i64_clonglong(n.value))
    }

    public init(_ n: UInt) {
        // assert (n <= UInt(self.max()))
        &self = CLongLong(value: Builtin.tocint_u_word_clonglong(n.value))
    }

    public init(_ n: Int) {
        &self = CLongLong(value: Builtin.tocint_s_word_clonglong(n.value))
    }
}