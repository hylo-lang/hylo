
trait Op {
  fun apply(x: Int) -> Int
}

type W : Op, Deinitializable {
  let m: Int
  internal memberwise init

  public fun apply(x: Int) -> Int {
    return x + m
  }
}

fun use<T: Op>(op: T, val: Int) -> Int {
  return op.apply(x: val)
}

public fun problem(a: Int, b: Int) -> Int {
  
  return use(op: W(m: a.copy()), val: b)
  
}
