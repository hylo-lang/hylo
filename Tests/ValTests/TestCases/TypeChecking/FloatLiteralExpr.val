//! expect-failure

fun check<T>(_ x: T) {}

public fun main() {
  let x0 = 4.2
  check<Double>(x0)

  let x1: Double = 4.2
  check<Double>(x0)

  let x2: Float = 4.2
  check<Float>(x2)

  let _: Double = x2  //! diagnostic cannot initialize object of type 'Float' with value of type 'Double'

  let _: Bool = 4.2   //! diagnostic type 'Bool' does not conform to trait 'ExpressibleByFloatLiteral'
}
