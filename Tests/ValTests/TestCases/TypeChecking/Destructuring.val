//! expect-failure

fun check<T>(_ x: T) {}

public fun main() {
  let (x0, y0) = (1, 2)
  check<Int>(x0)
  check<Int>(y0)
  
  //! @+1 diagnostic invalid destructuring of type 'Int'
  let (x1, y1): Int
  
  //! @+1 diagnostic invalid destructuring of type '{Int,}'
  let (x2, y2): {Int,}
  
  //! @+1 diagnostic tuples have different lengths
  let (x3, y3) = (1,)
}
