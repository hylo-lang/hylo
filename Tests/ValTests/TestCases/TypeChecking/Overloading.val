//! expect-failure

fun check<T>(_ x: T) {}

fun fn(x: Int) -> Int { fatal_error() }

fun fn(x: Double) -> Double { fatal_error() }

fun fn(x: Any) -> Any { fatal_error() }

fun fn<T>(x: T) -> T { fatal_error() }

fun fn<T>(_ x: T, _ y: T) -> T { fatal_error() }

fun fn<T>(_ x: T, _ y: Int) -> T { fatal_error() }

fun fn(_ x: Any, _ y: Any) -> Any { fatal_error() }

public fun main() {
  let x0 = fn(x: 1)
  check<Int>(x0)

  let x1 = fn(x: true)
  check<Bool>(x1)

  let x2: Int = fn(x: 1)
  check<Int>(x2)

  let x3: Double = fn(x: 1)
  check<Double>(x3)

  let x4: Double = fn(1.0, 1.0)
  check<Double>(x4)

  let x5 = fn(true, false)
  check<Bool>(x5)

  let x6 = fn(1, true)
  check<Any>(x6)

  let a = 1
  fn(a, a)    //! diagnostic ambiguous use of 'fn'
}
