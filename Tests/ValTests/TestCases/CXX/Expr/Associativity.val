fun test() {
  var a = 0
  var b = 0
  var c = 0
  var d = 0

  // left to right associativity, same operand

  //! cpp 1 + 2 + 3
  let _ = 1 + 2 + 3
  //! cpp 1 + 2 + 4
  let _ = (1 + 2) + 4
  //! cpp 1 + (2 + 4)
  let _ = 1 + (2 + 4)

  // left to right associativity, different operands with the same precedence

  //! cpp 1 + 2 - 3
  let _ = 1 + 2 - 3
  //! cpp 1 + 2 - 4
  let _ = (1 + 2) - 4
  //! cpp 1 + (2 - 4)
  let _ = 1 + (2 - 4)

  // right to left associativity, i.e., conditional operator

  //! cpp 1 == 0 ? (2 == 0 ? 3 : 4) : (2 == 0 ? 4 : 3)
  let _ = if (1 == 0) {
      if (2 == 0) { 3 } else { 4 }
    }
    else {
      if (2 == 0) { 4 } else { 3 }
    }
}
