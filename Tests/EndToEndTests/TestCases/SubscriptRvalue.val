//- compileAndRun expecting: success

type A: Deinitializable {

  public memberwise init

  public subscript(x: Int): Int { x + 1 }

}

type B: Deinitializable {

  public memberwise init

  public var a: A

}

fun f() -> B { .new(a: .new()) }

fun f(_ x: Void) -> A { .new() }

public fun main() {
  precondition(f().a[x: 1] == 2)
}
