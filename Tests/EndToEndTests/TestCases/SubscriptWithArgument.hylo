//- compileAndRun expecting: success

public type A: Deinitializable {

  public var x: Int

  public memberwise init

  public subscript(y: Int): Int {
    x + y
  }

}

subscript offset(_ x: Int, by y: Int): Int {
  x + y
}

public fun main() {
  let a: A = .new(x: 1)
  let b = a[y: 1]
  precondition(b == 2)

  let c = offset[a.x, by: 1]
  precondition(c == 2)
}
