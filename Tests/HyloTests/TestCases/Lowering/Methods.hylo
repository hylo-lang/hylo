extension Int {
  public fun foo() let -> Int { copy() }
  public fun bar() inout { &self = -self }
  public fun ham() sink -> Int { self }
  public fun viz(_ v: sink Int) set { &self = v }
}

public fun main() {
  var x = 5
  &x = x.foo()
  &x.bar()
  let y = x.ham()
  &x.viz(y)
}
